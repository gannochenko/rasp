{
    "name": "mail-checker",
    "version": "1.0.0",
    "scripts": {
        "start": "node ./index.js",
        "dev": "webpack --config webpack.js --mode development --watch",
        "test": "jest -c ./jest/config.unit.js --forceExit",
        "test:watch": "jest -c ./jest/config.unit.js --watch",
        "test:debug": "node --inspect-brk=0.0.0.0:4001 -r tsconfig-paths/register -r ./node_modules/.bin/jest -c ./jest/config.unit.js  --runInBand",
        "test:int": "jest -c ./jest/config.int.js --runInBand --forceExit --passWithNoTests",
        "test:int:watch": "jest -c ./jest/config.int.js --runInBand --watch",
        "build": "rm -rf ./build && webpack --config webpack.js --mode production",
        "migrate": "ts-node ./node_modules/typeorm/cli.js migration:run",
        "migrate:local": "./script/migrate.sh local",
        "lint": "eslint ./src/ --ext .js,.ts",
        "lint:fix": "eslint ./src/ --fix --ext .js,.ts",
        "techdebt": "leasot --exit-nicely --reporter table --skip-unsupported --ignore node_modules,build,migrations ./",
        "image:build": "./script/image.build.sh",
        "image:push": "./script/image.push.sh"
    },
    "keywords": [],
    "author": "gannochenko.sv@gmail.com",
    "license": "MIT",
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged"
        }
    },
    "private": true,
    "dependencies": {
        "@babel/polyfill": "^7.8.7",
        "@gannochenko/etc": "^1.0.3",
        "@gannochenko/express.mvc": "^1.0.0",
        "body-parser": "^1.19.0",
        "clone-deep": "^4.0.1",
        "cors": "^2.8.5",
        "debug": "^4.1.1",
        "express": "^4.17.1",
        "grpc": "^1.24.2",
        "helmet": "^3.22.0",
        "moment": "^2.25.3",
        "pg": "^8.1.0",
        "prom-client": "^12.0.0",
        "response-time": "^2.3.2",
        "typeorm": "^0.2.24"
    },
    "devDependencies": {
        "@babel/core": "^7.9.6",
        "@babel/plugin-proposal-class-properties": "^7.8.3",
        "@babel/plugin-proposal-decorators": "^7.8.3",
        "@babel/plugin-proposal-object-rest-spread": "^7.9.6",
        "@babel/plugin-transform-runtime": "^7.9.6",
        "@babel/preset-env": "^7.9.6",
        "@babel/preset-typescript": "^7.9.0",
        "@grpc/proto-loader": "^0.5.4",
        "@types/clone-deep": "^4.0.1",
        "@types/cors": "^2.8.6",
        "@types/helmet": "^0.0.46",
        "@types/jest": "^25.2.1",
        "@types/node": "^13.13.5",
        "@types/response-time": "^2.3.4",
        "@typescript-eslint/eslint-plugin": "^2.32.0",
        "@typescript-eslint/parser": "^2.32.0",
        "babel-eslint": "^10.1.0",
        "babel-loader": "^8.1.0",
        "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
        "copy-webpack-plugin": "^5.1.1",
        "eslint": "^7.0.0",
        "eslint-config-airbnb-base": "^14.1.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-loader": "^4.0.2",
        "eslint-plugin-import": "^2.20.2",
        "eslint-plugin-prettier": "^3.1.3",
        "fork-ts-checker-webpack-plugin-alt": "^0.4.14",
        "husky": "^4.2.5",
        "jest": "^26.0.1",
        "leasot": "^10.4.0",
        "nodemon": "^2.0.3",
        "nodemon-webpack-plugin": "^4.3.1",
        "prettier": "^2.0.5",
        "pretty-quick": "^2.0.1",
        "supertest": "^4.0.2",
        "ts-essentials": "^6.0.4",
        "ts-jest": "^25.5.1",
        "ts-loader": "^7.0.4",
        "ts-node": "^8.10.1",
        "typescript": "^3.8.3",
        "webpack": "^4.43.0",
        "webpack-cli": "^3.3.11",
        "webpack-merge": "^4.2.2",
        "webpack-node-externals": "^1.7.2"
    }
}
